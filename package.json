{
	"name": "error.iserror",
	"version": "1.0.0",
	"description": "An ESnext spec-compliant `Error.isError` shim/polyfill/replacement that works as far down as ES3",
	"main": "index.js",
	"browser": {
		"util": false
	},
	"exports": {
		".": "./index.js",
		"./auto": "./auto.js",
		"./polyfill": "./polyfill.js",
		"./implementation": "./implementation.js",
		"./shim": "./shim.js",
		"./package.json": "./package.json"
	},
	"scripts": {
		"prepack": "npmignore --auto --commentLines=autogenerated",
		"prepublish": "not-in-publish || npm run prepublishOnly",
		"prepublishOnly": "safe-publish-latest",
		"pretest": "npm run --silent lint",
		"test": "npm run tests-only",
		"posttest": "npx npm@'>=10.2' audit --production",
		"tests-only": "nyc tape 'test/**/*.js'",
		"prelint": "evalmd README.md",
		"lint": "eslint --ext=js,mjs .",
		"postlint": "es-shim-api --type=function",
		"version": "auto-changelog && git add CHANGELOG.md",
		"postversion": "auto-changelog && git add CHANGELOG.md && git commit --no-edit --amend && git tag -f \"v$(node -e \"console.log(require('./package.json').version)\")\""
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/es-shims/Error.isError.git"
	},
	"keywords": [
		"javascript",
		"ecmascript",
		"polyfill",
		"shim",
		"Error",
		"isError",
		"es-shim API"
	],
	"author": "Jordan Harband <ljharb@gmail.com>",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/es-shims/Error.isError/issues"
	},
	"homepage": "https://github.com/es-shims/Error.isError#readme",
	"dependencies": {
		"define-properties": "^1.2.1",
		"es-errors": "^1.3.0",
		"gopd": "^1.0.1",
		"has-tostringtag": "^1.0.2"
	},
	"devDependencies": {
		"@es-shims/api": "^3.0.1",
		"@ljharb/eslint-config": "^21.1.1",
		"@types/node": "^22.7.5",
		"auto-changelog": "^2.5.0",
		"call-bind": "^1.0.7",
		"es-value-fixtures": "^1.5.0",
		"eslint": "=8.8.0",
		"evalmd": "^0.0.19",
		"for-each": "^0.3.3",
		"functions-have-names": "^1.2.3",
		"iframe": "^1.0.0",
		"in-publish": "^2.0.1",
		"npmignore": "^0.3.1",
		"nyc": "^10.3.2",
		"object-inspect": "^1.13.2",
		"safe-publish-latest": "^2.0.0",
		"tape": "^5.9.0"
	},
	"testling": {
		"files": "test/index.js"
	},
	"engines": {
		"node": ">= 0.4"
	},
	"auto-changelog": {
		"output": "CHANGELOG.md",
		"template": "keepachangelog",
		"unreleased": false,
		"commitLimit": false,
		"backfillLimit": false,
		"hideCredit": true
	},
	"publishConfig": {
		"ignore": [
			".github/workflows"
		]
	}
}
